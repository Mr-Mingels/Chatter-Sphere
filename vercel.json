{
    "version": 2,
    "builds": [
      { "src": "api/server.js", "use": "@vercel/node" },
      { "src": "package.json", "use": "@vercel/static-build", "config": {"distDir": "build"} }
    ],
    "routes": [
        { "src": "/api/.*", "dest": "api/server.js" },
        { "src": "/static/.*", "dest": "/static/$1" },
        { "src": "/(.*).(js|css)", "dest": "/$1.$2" },
        { "src": "/.*", "dest": "/index.html" }
    ]  
}

  
  